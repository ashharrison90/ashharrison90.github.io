    "vitest-fail-on-console": "^0.7.0",
    "webpack": "^5.94.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --ext .ts,.tsx --fix"
    ],
    "*.scss": "stylelint --fix",
    "*": [
      "prettier --write"
    ]
  }
}
